{
    "name": "alhoqbani/slim-basic",
    "description": "Slim Basic.",
    "keywords": ["framework", "Slim"],
    "license": "MIT",
    "type": "project",
    "require": {
        "slim/slim": "^3.8",
        "slim/twig-view": "^2.2",
        "vlucas/phpdotenv": "^2.4",
        "respect/validation": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
